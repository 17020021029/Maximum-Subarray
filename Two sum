/**
 * Note: The returned array must be malloced, assume caller calls free().
 */
int* twoSum(int* nums, int numsSize, int target) {
    static int result[2];
    int *p;
    p=result;
    int m,n;
    for(int i=0;i<numsSize;i++)
    {
        for(int j=1;j<numsSize;j++)
        {
            if((*(nums+i)+*(nums+j))==target&&(i<j)){
                result[0]=i;
                result[1]=j;
            }
        }
    }
    return p;
}
